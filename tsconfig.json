{
    "compilerOptions": {
        "module": "commonjs",
        "noEmit": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "skipLibCheck": true,
        "strict": true,
        "strictFunctionTypes": true,
        "target": "es2016"
    },
    "include": ["src/**/*", "__tests__/**/*"]
}
